{
    "openapi":"3.0.2",
    "info": {
      "title":"User API",
      "description": "User API",
      "version":"1.0.0"
    },
    "servers": [
      {"url":"https://localhost:3000"}
    ],
    "paths": {
        "/api/products": {
            "get": {
                "description": "Get list of products.",
                "parameters": [
                    {
                        "schema": {
                            "type": "string"
                        },
                        "name":"Authorization",
                        "in":"header"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success get list of products.",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "type":"object",
                                    "properties":{
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "id": {
                                                    "type": "number"
                                                },
                                                "username": {
                                                    "type": "string"
                                                },
                                                "name": {
                                                    "type": "string"
                                                },
                                                "token":{
                                                    "type":"string"
                                                }
                                            } 
                                        },
                                        "errors": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/products/:id": {
            "get": {
                "description": "Get product by ID.",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            },
            "put": {
                "description": "",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            },
            "delete": {
                "description": "",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
    }
  }